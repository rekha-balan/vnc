    <dxlc:LayoutControl Orientation="Vertical">

        <dxlc:LayoutGroup Header="Find ImportsStatementSyntax" 
        	View="GroupBox"
            Orientation="Vertical"
            IsCollapsible="True" IsCollapsed="True">

            <dxlc:LayoutGroup
