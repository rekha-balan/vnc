    <dxlc:LayoutControl Orientation="Vertical" MinWidth="300">

        <dxlc:LayoutGroup Header="Find ImportsStatement Syntax" 
        	View="GroupBox"
            Orientation="Vertical"
            IsCollapsible="True" IsCollapsed="True">

            <dxlc:LayoutGroup
            	Orientation="Horizontal">

                <dxlc:LayoutGroup
                	Orientation="Vertical">
                    <dxlc:LayoutItem>
                        <dxe:CheckEdit x:Name="ceImportsStatementUseRegEx" Content="Use Regular Expressions"
                                       IsChecked="False"/>
                    </dxlc:LayoutItem>

                    <dxlc:LayoutItem Label="Imports">
                        <dxe:TextEdit x:Name="teImportsStatementRegEx" Text=".*"
                                      IsEnabled="{cb:Binding ElementName=ceImportsStatementUseRegEx, Path=IsChecked, Mode=OneWay}"
                                      ToolTip="Supports Regular Expressions"/>
                    </dxlc:LayoutItem>

                </dxlc:LayoutGroup>

            </dxlc:LayoutGroup>

            <dxlc:LayoutItem>
                <Button Content="ImportsStatement Walker" 
                        Style="{StaticResource SyntaxQueryButton}"
                        Click="btnImportsStatementWalker_Click" />
            </dxlc:LayoutItem>

        </dxlc:LayoutGroup>

