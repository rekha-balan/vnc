    <Setter Property="Fill">
      <Setter.Value>
        <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1" cs:Name="LinearGradientBrush_0012">
          <GradientStop Color="#FFB9B9B9" Offset="0" cs:Name="GradientStop_0001" />
          <GradientStop Color="#FFB9B9B9" Offset="1" cs:Name="GradientStop_0002" />
        </LinearGradientBrush>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>