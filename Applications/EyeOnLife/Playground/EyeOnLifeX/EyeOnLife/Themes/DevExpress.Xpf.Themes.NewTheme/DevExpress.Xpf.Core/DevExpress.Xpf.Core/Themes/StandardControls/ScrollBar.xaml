        <Setter Property="Background" Value="#00000000" cs:Name="Setter_0007" />
        <Setter Property="Foreground" Value="#00000000" cs:Name="Setter_0008" />
        <Setter Property="BorderBrush" Value="#00000000" cs:Name="Setter_0009" />
