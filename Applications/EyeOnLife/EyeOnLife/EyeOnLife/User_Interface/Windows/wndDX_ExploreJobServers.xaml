<dx:DXWindow
    x:Class="EyeOnLife.User_Interface.Windows.wndDX_ExploreJobServers"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" 

    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"      
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dxdb="http://schemas.devexpress.com/winfx/2008/xaml/demobase"
    xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking"  
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
    xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"     

    xmlns:local="clr-namespace:EyeOnLife.User_Interface"
    xmlns:localData="clr-namespace:SQLInformation.Data;assembly=SQLInformation"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"       
    xmlns:sys="clr-namespace:System;assembly=mscorlib"   
    
    mc:Ignorable="d"
    Title="Instance-JobServer Explorer" FontSize="16"
    d:DesignWidth="900" d:DesignHeight="600" 
    WindowStartupLocation="CenterScreen"
    MinWidth="900" MinHeight="600"
    Loaded="DXWindow_OnLoaded" SizeChanged="DXWindow_OnSizeChanged" >

    <dx:DXWindow.Resources>
        
        <localData:ApplicationDataSet x:Key="applicationDataSet" />
        
        <CollectionViewSource x:Key="instancesViewSource" Source="{Binding Instances, Source={StaticResource applicationDataSet}}"/>
        <CollectionViewSource x:Key="instancesJobServersViewSource" Source="{Binding FK_JobServers_Instances, Source={StaticResource instancesViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSAlertCategoriesViewSource" Source="{Binding FK_JSAlertCategories_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSAlertsViewSource" Source="{Binding FK_JSAlerts_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSJobCategoriesViewSource" Source="{Binding FK_JSJobCategories_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSOperatorCategoriesViewSource" Source="{Binding FK_JSOperatorCategories_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSOperatorsViewSource" Source="{Binding FK_JSOperators_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSProxyAccountsViewSource" Source="{Binding FK_JSProxyAccounts_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSTargetServerGroupsViewSource" Source="{Binding FK_JSTargetServerGroups_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSTargetServersViewSource" Source="{Binding FK_JSTargetServers_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSJobsViewSource" Source="{Binding FK_JSJobs_JobServers, Source={StaticResource instancesJobServersViewSource}}"/>
        <CollectionViewSource x:Key="instancesJobServersJSJobsJSJobStepsViewSource" Source="{Binding FK_JSJobSteps_JSJobs, Source={StaticResource instancesJobServersJSJobsViewSource}}"/>

        
        <ControlTemplate x:Key="AdminWindowToolBoxControlsTemplate" >

            <StackPanel>
                <Button x:Name="saveButton" Margin="5" Content="Save" Click="saveButton_Click"  />
                <Button x:Name="undoButton" Margin="5" Content="Undo" Click="undoButton_Click" />

                <!--<CheckBox x:Name="readOnlyCheckBox" Margin="5"
                            Content="Read only" IsChecked="False"
                            Checked="readOnlyCheckBox_Checked"  Unchecked="readOnlyCheckBox_UnChecked" />

                <CheckBox x:Name="canAddCheckBox" Margin="5"                            
                            Content="Can add" IsChecked="True"
                            Checked="canAddCheckBox_Checked" Unchecked="canAddCheckBox_UnChecked" />

                <CheckBox x:Name="canDeleteCheckBox" Margin="5"
                            Content="Can delete" IsChecked="True"
                            Checked="canDeleteCheckBox_Checked" Unchecked="canDeleteCheckBox_UnChecked" />-->

                <CheckBox Name="ckDisplayIDColumns" 
                              Content="Display ID Columns" 
                              Margin="5" 
                              Checked="OnDisplayIDColumns_Checked" Unchecked="ckDisplayIDColumns_Unchecked"/>

                <CheckBox Name="ckDisplaySnapShotColumns" 
                              Content="Display SnapShot Columns" 
                              Margin="5"                          
                              Checked="OnDisplaySnapShotColumns_Checked" Unchecked="ckDisplaySnapShotColumns_Unchecked"/>
            </StackPanel>

        </ControlTemplate>

        <Style x:Key="BoldFontLabelStyle" TargetType="dxlc:LayoutItemLabel">
            <Setter Property="FontWeight" Value="Bold" />
        </Style>

    </dx:DXWindow.Resources>

    <dxlc:LayoutControl Name="lc_Root" 
                      Orientation="Vertical"
                      Background="Transparent"
                      VerticalAlignment="Stretch"
                      DataContext="">
        
        <dxlc:LayoutControl.AvailableItems>
            
            <!-- Add things that are optionally displayable-->

        </dxlc:LayoutControl.AvailableItems>

        <dxlc:LayoutGroup Name="lg_Header"
                        VerticalAlignment="Top">

            <dxlc:LayoutGroup 
                Orientation="Horizontal" 
                HorizontalAlignment="Left"
                VerticalAlignment="Center"
                ItemSpace="30">

                <!--<dxlc:LayoutItem Label="Server">
                    <TextBlock Text="{Binding Path=NetName}" />
                </dxlc:LayoutItem>-->

                <dxlc:LayoutItem Label="Instance"
                                 DataContext="{StaticResource instancesViewSource}">
                    <TextBlock Text="{Binding Path=Name_Instance}" />
                </dxlc:LayoutItem>

                <!--<dxlc:LayoutItem Label="Database"
                                 DataContext="{StaticResource serversInstancesDatabasesViewSource}">
                    <TextBlock Text="{Binding Path=Name_Database}" />
                </dxlc:LayoutItem>-->

            </dxlc:LayoutGroup>

            <dxlc:LayoutGroup 
                Orientation="Vertical" 
                HorizontalAlignment="Right"
                VerticalAlignment="Center" >

                <RadioButton Content="Show as Tabs" Name="checkShowAsTabs"
                             IsChecked="True" 
                             Checked="ShowAs_Checked" 
                             FontSize="10" />
                <RadioButton Content="Show as Group Boxes" Name="checkShowAsGroupBoxes" 
                             Checked="ShowAs_Checked" 
                             FontSize="10" />

            </dxlc:LayoutGroup>

        </dxlc:LayoutGroup>
        
