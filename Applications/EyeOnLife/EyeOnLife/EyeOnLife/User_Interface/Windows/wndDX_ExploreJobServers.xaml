                                                <dxlc:LayoutItem Label="SnapShotError: ">
                                                    <TextBlock Text="{Binding SnapShotError}"  />
                                                </dxlc:LayoutItem>

                                            </dxlc:LayoutControl>

                                            <dxlc:LayoutControl Orientation="Vertical">

                                                <!--<dxlc:LayoutItem Label="BrowserStartMode: ">
                                                    <TextBlock Text="{Binding BrowserStartMode}"  />
                                                </dxlc:LayoutItem>-->

                                            </dxlc:LayoutControl>

                                        </dxlc:LayoutGroup>

                                    </dxlc:LayoutItem>
                                    
                                </dxlc:LayoutGroup>

                                <dxlc:LayoutGroup Header="JS AlertCategories" Name="layoutGroupJSAlertCategories"
                                    VerticalAlignment="Top"
                                    MaxHeight="1200"
                                    View="Group" 
                                    Orientation="Vertical" 
