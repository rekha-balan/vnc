                                            DataContext="{Binding Path=View.FocusedRowData.Row, ElementName=fK_Instances_ServersGridControl}">

                                            <dxlc:LayoutControl Orientation="Vertical">

                                                <dxlc:LayoutItem Label="SnapShotDate: ">
                                                    <TextBlock Text="{Binding SnapShotDate}"  />
                                                </dxlc:LayoutItem>

                                                <dxlc:LayoutItem Label="SnapShotError: ">
                                                    <TextBlock Text="{Binding SnapShotError}"  />
                                                </dxlc:LayoutItem>

                                                <dxlc:LayoutItem Label="IsMonitored: ">
                                                    <TextBlock Text="{Binding IsMonitored}"  />
                                                </dxlc:LayoutItem>

                                            </dxlc:LayoutControl>

                                            <dxlc:LayoutControl Orientation="Vertical">

                                                <dxlc:LayoutItem Label="BrowserStartMode: ">
                                                    <TextBlock Text="{Binding BrowserStartMode}"  />
                                                </dxlc:LayoutItem>

                                                <dxlc:LayoutItem Label="Collation: ">
                                                    <TextBlock Text="{Binding Collation}"  />
                                                </dxlc:LayoutItem>

                                                <dxlc:LayoutItem Label="EngineEdition: ">
                                                    <TextBlock Text="{Binding EngineEdition}"  />
                                                </dxlc:LayoutItem>

                                            </dxlc:LayoutControl>

                                        </dxlc:LayoutGroup>

                                    </dxlc:LayoutItem>

                                </dxlc:LayoutGroup>

                                <dxlc:LayoutGroup Header="JobServer" Name="layoutGroupJobServer"
                                    VerticalAlignment="Top"
                                    MaxHeight="1200"
                                    View="Group" 
                                    Orientation="Vertical" 
                                    IsCollapsible="True"
                                    IsCollapsed="False"  >

                                    <dxlc:LayoutItem Name="layoutItem_JobServers" MaxHeight="1200" VerticalAlignment="Top">
                                        
