    <ControlTemplate x:Key="MasterListHeaderTemplate">
        <Grid Name="grid_roster_top" 
              Column="1" ColumnSpan="1" Row="1" RowSpan="1"  
              HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="Auto" Height="Auto">
            <StackPanel Orientation="Horizontal">
                <TextBlock Name="serverList"  
                           HorizontalAlignment="Left" VerticalAlignment="Center" Margin="20,0,10,0"  
                           Height="Auto" FontSize="14" FontFamily="{StaticResource FontFamilyBrand1}" 
                           FontStyle="Normal" FontWeight="UltraBold" 
                           Foreground="sc#1.000000, 0.167488, 0.167488, 0.167480" 
                           Text="Master List" />
                <TextBlock Name="serverListCount" 
                           HorizontalAlignment="Left" VerticalAlignment="Center" 
                           Width="Auto" Height="Auto" FontSize="18" 
                           FontFamily="{StaticResource FontFamilyBrand1}" 
                           FontStyle="Normal" FontWeight="UltraBold"
                           Foreground="sc#1.000000, 0.167000, 0.167000, 0.167000" 
                           Text="{Binding ElementName=MasterList, Path=Items.Count}" />
            </StackPanel>
        </Grid>
    </ControlTemplate>  
    
    <Style x:Key="MasterListStyleComboBoxItem" TargetType="{x:Type ComboBoxItem}">
        <Setter Property="SnapsToDevicePixels" Value="true"/>
        <Setter Property="OverridesDefaultStyle" Value="true"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ComboBoxItem}">
                    <Border Width="20"
					  Name="Border" Opacity="0.5"
         
					  SnapsToDevicePixels="true">
                        <ContentPresenter />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="Border" Property="Opacity" Value="1"/>
                        </Trigger>
                        <Trigger Property="IsHighlighted" Value="true">
                            <!--<Setter TargetName="Border" Property="Background" Value="{StaticResource SelectedBackgroundBrush}"/>-->
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Foreground" Value="{StaticResource DisabledForegroundBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>    
    
<!-- This controls the style of the ComboBox that allows selection of the MasterList Style -->
    <ControlTemplate x:Key="MasterListStyleSelectorTemplate">
        <Grid>
            <ComboBox x:Name="MasterSelectorComboBox" Width="20" Height="20"  Style="{DynamicResource MasterListStyleSelectorComboBox}"  SelectedIndex="0">
                <ComboBoxItem Style="{StaticResource MasterListStyleComboBoxItem}"  Margin="0,5,0,0" >
                    <Grid>
                        <Ellipse Fill="Blue" Height="20" Width="20" >
                            <Ellipse.Stroke>
                                <LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
                                    <GradientStop Color="#FF000000" Offset="0"/>
                                    <GradientStop Color="#FFFFFFFF" Offset="1"/>
                                </LinearGradientBrush>
                            </Ellipse.Stroke>
                        </Ellipse>
                        <Rectangle Fill="white" Height="2" Width="10" />
                    </Grid>
                </ComboBoxItem>
                <ComboBoxItem Style="{StaticResource MasterListStyleComboBoxItem}" Margin="0,5,0,0" >
                    <Grid>
                        <Ellipse Fill="Blue" Height="20" Width="20" >
                            <Ellipse.Stroke>
                                <LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
                                    <GradientStop Color="#FF000000" Offset="0"/>
                                    <GradientStop Color="#FFFFFFFF" Offset="1"/>
                                </LinearGradientBrush>
                            </Ellipse.Stroke>
                        </Ellipse>
                        <Rectangle Fill="white" Height="2" Width="Auto" VerticalAlignment="Top" Margin="5,7,5,0" />
                        <Rectangle Fill="white" Height="2" Width="Auto" VerticalAlignment="Bottom" Margin="5,0,5,7" />
                    </Grid>
                </ComboBoxItem>
                <ComboBoxItem Style="{StaticResource MasterListStyleComboBoxItem}" Margin="0,5,0,0" >
                    <Grid>
                        <Ellipse Fill="Blue" Height="20" Width="20" >
                            <Ellipse.Stroke>
                                <LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
                                    <GradientStop Color="#FF000000" Offset="0"/>
                                    <GradientStop Color="#FFFFFFFF" Offset="1"/>
                                </LinearGradientBrush>
                            </Ellipse.Stroke>
                        </Ellipse>
                        <Rectangle Fill="white" Height="2" Width="Auto" VerticalAlignment="Bottom" Margin="5,0,5,5" />
                        <Rectangle Fill="white" Height="2" Width="10" />
                        <Rectangle Fill="white" Height="2" Width="Auto"  VerticalAlignment="Top" Margin="5,5,5,0"  />
                    </Grid>
                </ComboBoxItem>
            </ComboBox>
        </Grid>
    </ControlTemplate>    
    
