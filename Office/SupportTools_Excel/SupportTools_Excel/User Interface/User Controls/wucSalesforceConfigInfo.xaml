<UserControl x:Class="SupportTools_Excel.User_Interface.User_Controls.wucSalesforceConfigInfo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>

        <Label Content="Status" />
        <StackPanel Margin="10,0,0,0">
            <Label Content="New" />
            <Label Content="In Progress" />
            <Label Content="OnHold" />
            <Label Content="Waiting for Internal Resource" />
            <Label Content="Waiting for External Resource" />
            <Label Content="Closed" />
        </StackPanel>
        
        <Label Content="Product Status" />
        <StackPanel Margin="10,0,0,0">
            <Label Content="New" />
            <Label Content="Evaluating" />
            <Label Content="Ready for Triage" />
            <Label Content="Assigned to ELT" />
            <Label Content="Assigned to Business" />
            <Label Content="Assigned to Product Management" />
            <Label Content="Assigned to Development" />
            <Label Content="Assinged to Product Support" />
            <Label Content="Waitng for Acceptance" />
            <Label Content="Waiting for Data" />
            <Label Content="Waiting for Infrastructure" />
            <Label Content="Waiting for QA" />
            <Label Content="Waiting for Request Clarification" />
            <Label Content="Waiting for Requirements Elaboration" />
            <Label Content="Sizing" />
            <Label Content="Prioritizing" />
            <Label Content="Scheduling" />
            <Label Content="Added to Roadmap" />
            <Label Content="Added to Backlog" />
            <Label Content="Assigned to Iteration" />
            <Label Content="Assigned to Queue" />
        </StackPanel>
    </StackPanel>
</UserControl>
